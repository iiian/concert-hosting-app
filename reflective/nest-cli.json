{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/reflective/src",
  "monorepo": true,
  "root": "apps/reflective",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/reflective/tsconfig.app.json"
  },
  "projects": {
