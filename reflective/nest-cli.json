    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "credit": {
      "type": "application",
      "root": "apps/credit",
      "entryFile": "main",
      "sourceRoot": "apps/credit/src",
      "compilerOptions": {
